{
  "name": "xlsx-template-next",
  "version": "0.0.2",
  "description": "Generate .xlsx (Excel) files from templates built in Excel",
  "main": "./build/index",
  "files": [
    "build"
  ],
  "author": {
    "name": "Martin Aspeli"
  },
  "contributors": [
    {
      "name": "Martin Aspeli",
      "email": "optilude@gmail.com"
    },
    {
      "name": "Andrii Kurdiumov",
      "email": "kant2002@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mirror-ru/xlsx-template.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/optilude/xlsx-template/raw/master/LICENSE"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./test/coverage",
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "transform": {
      ".(js|ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|-(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testTimeout": 500,
    "moduleNameMapper": {
      "^app/(.*)": "<rootDir>/build/$1",
      "^tests/(.*)": "<rootDir>/tests/$1"
    }
  },
  "dependencies": {
    "elementtree": "^0.1.7",
    "image-size": "^1.0.2",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "compile": "tsc",
    "test": "npm run compile && jest",
    "prepublishOnly": "npm run compile"
  }
}
